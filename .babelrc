{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory":"lib",
                "style": true
            },
            "antd"
        ],
        [
            "module-resolver",
            {
                "alias": {
                    "dva": "dva-no-router"
                }
            }
        ]
    ],
    "ignore": [
        "node_modules"
    ],
    "exclude": ["node_modules"]
}